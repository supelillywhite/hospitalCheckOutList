<div class="navigation-my-styles">
  <div class="floating-box"><%= link_to 'Home', root_path %></div>
  <div class="floating-box"><%= link_to 'Patients', patients_path %></div>
  <div class="floating-box"><% if current_user %>
    <%= link_to "Logout", destroy_user_session_path, method: :delete %>
    <% else %>
        <%= link_to "Register", new_user_registration_path %>
        <%= link_to "Login", new_user_session_path %>
    <% end %>
  </div class="floating-box">
  <% if current_user != nil %>
    <p>Welcome <%= current_user.name %>!</p>
  <% else %>
    <p>Please log in!</p>
  <% end %>
</div>